<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">  
    
    <!-- dilshi -->
         
	<servlet>
		<servlet-name>loginS</servlet-name>
		<servlet-class>com.user.LoginServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>loginS</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	     		
	
	<servlet>
		<servlet-name>registerS</servlet-name>
		<servlet-class>com.customer.RegisterServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>registerS</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
		
	
	<servlet>
		<servlet-name>updateS</servlet-name>
		<servlet-class>com.customer.UpdateCustomerServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>updateS</servlet-name>
		<url-pattern>/updatecus</url-pattern>
	</servlet-mapping>
		
	
	<servlet>
		<servlet-name>deleteS</servlet-name>
		<servlet-class>com.customer.DeleteCustomerServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>deleteS</servlet-name>
		<url-pattern>/deletecus</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>updateA</servlet-name>
		<servlet-class>com.admin.UpdateAdminServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>updateA</servlet-name>
		<url-pattern>/updateadm</url-pattern>
	</servlet-mapping>
		
	
	<servlet>
		<servlet-name>contactS</servlet-name>
		<servlet-class>com.contact.ContactServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>contactS</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>
	

  	
  	<!-- chamath -->
  	
  	<servlet> 
        <servlet-name>requestInsert </servlet-name>
        <servlet-class>com.Event_Request.requestInsertServlet</servlet-class>
    </servlet>
         
    <servlet-mapping>
         <servlet-name>requestInsert </servlet-name>
         <url-pattern>/insertrequest</url-pattern>
    </servlet-mapping>
    
    
	<servlet>
	      <servlet-name>upddateRequest </servlet-name>
	      <servlet-class>com.Event_Request.updateRequestServlet</servlet-class>
    </servlet>
         
    <servlet-mapping>
          <servlet-name>upddateRequest </servlet-name>
          <url-pattern>/updateevent</url-pattern>
    </servlet-mapping>
         
         
    <servlet>
	      <servlet-name>deleteRequest </servlet-name>
	      <servlet-class>com.Event_Request.deleteRequestServlet</servlet-class>
    </servlet>
         
    <servlet-mapping>
          <servlet-name>deleteRequest </servlet-name>
          <url-pattern>/deleteevent</url-pattern>
    </servlet-mapping> 
   
 
  
 </web-app>
  
  
    